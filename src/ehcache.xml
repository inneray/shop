<ehcache>
    
    <diskStore path="java.io.tmpdir"/>

  <!-- 对象替换算法 ： FIFO 先进先出    LRU时间优先策略  LFU频率优先策略  -->
    <defaultCache
            maxElementsInMemory="1000"
            eternal="false"
            timeToIdleSeconds="100"
            timeToLiveSeconds="200"
            overflowToDisk="true"
            memoryStoreEvictionPolicy="LFU"
            />
</ehcache>
